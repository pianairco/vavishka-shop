<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class
    name="ir.piana.dev.strutser.data.model.Sms"
    table="SMS"
    >

    <id
      name="smsId"
      type="java.lang.Long"
      column="SMS_ID"
      >
      <generator class="native" />
    </id>

    <property
      name="fromNumber"
      type="java.lang.String"
      column="FROM_NUMBER"
      not-null="true"
      length="32"
      />
    <property
      name="toNumber"
      type="java.lang.String"
      column="TO_NUMBER"
      not-null="true"
      length="32"
      />
    <property
      name="content"
      type="java.lang.String"
      column="CONTENT"
      not-null="true"
      length="4000"
      />
    <property
      name="requestDate"
      type="java.lang.String"
      column="REQUEST_DATE"
      not-null="true"
      length="10"
      />
    <property
      name="requestTime"
      type="java.lang.String"
      column="REQUEST_TIME"
      not-null="true"
      length="5"
      />
    <property
      name="sendStatus"
      type="long"
      column="SEND_STATUS"
      not-null="true"
      length="18"
      />
    <property
      name="smsType"
      type="long"
      column="SMS_TYPE"
      not-null="true"
      length="18"
      />
    <property
      name="sendDate"
      type="java.lang.String"
      column="SEND_DATE"
      not-null="false"
      length="10"
      />
    <property
      name="sendTime"
      type="java.lang.String"
      column="SEND_TIME"
      not-null="false"
      length="5"
      />
    <property
      name="errorMessage"
      type="java.lang.String"
      column="ERROR_MESSAGE"
      not-null="false"
      length="4000"
      />
    <property
      name="response"
      type="java.lang.String"
      column="RESPONSE"
      not-null="false"
      length="4000"
      />

  </class>
</hibernate-mapping>
